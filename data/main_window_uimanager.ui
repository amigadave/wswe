<ui>
  <menubar name="MainMenu">
    <menu name="FileMenu" action="FileMenuAction">
      <menuitem name="Open" action="OpenAction" />
      <menuitem name="Save" action="SaveAction" />
      <separator/>
      <menuitem name="Quit" action="QuitAction" />
    </menu>
    <menu name="GoMenu" action="GoMenuAction">
      <menuitem name="Add place..." action="AddPlaceAction" />
      <menuitem name="Remove place" action="RemovePlaceAction" />
      <separator/>
      <menuitem name="Add visit..." action="AddVisitAction" />
      <menuitem name="Remove visit" action="RemoveVisitAction" />
      <separator/>
      <menuitem name="Choose place" action="ChoosePlaceAction" />
    </menu>
    <menu name="HelpMenu" action="HelpMenuAction">
      <menuitem name="About" action="AboutAction" />
    </menu>
  </menubar>

  <toolbar name="MainToolbar" action="MainMenuBarAction">
    <placeholder name="PlaceToolItems">
      <separator/>
      <toolitem name="Add place" action="AddPlaceAction" />
      <toolitem name="Remove place" action="RemovePlaceAction" />
      <separator/>
      <toolitem name="Add visit" action="AddVisitAction" />
      <toolitem name="Remove visit" action="RemoveVisitAction" />
      <separator/>
      <toolitem name="Choose place" action="ChoosePlaceAction" />
      <separator/>
    </placeholder>
  </toolbar>
</ui>
